{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "useDefineForClassFields": false,
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "verbatimModuleSyntax": true,
        "moduleDetection": "force",
        "types": [
            "node"
        ],
        "rewriteRelativeImportExtensions": true,
        "erasableSyntaxOnly": false,
        "baseUrl": "."
    }
}